#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <bits/stdc++.h>
using namespace std;


int main(){
    int N;
    int a;
    cin >> N;
    vector <int> v1;
    for (int i=0; i<N; i++){
        cin >> a;
        v1.push_back(a);
        }
    int M;
    int b;
    vector <int> v2;
    cin >> M;
    for(int j=0; j<M; j++){
        cin >> b;
        v2.push_back(b);
        }
    int count;
    for(int k=0; k<M; k++){
        for(int g=0; g<N; g++){
            if(v2[k]==v1[g]){
                count++;
                }
            }
        cout << count << endl;
        count = 0;
        }
    return 0;
    }
