#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <bits/stdc++.h>

using namespace std;
int n;
queue<int> q;
vector<int> dis;

int main(){
	while(cin>>n){
		if(n==0) break;
		dis.clear();
		while(!q.empty()) q.pop();
		for(int i=1;i<=n;i++) q.push(i);
		while(q.size() > 1){
			dis.push_back(q.front());
			q.pop();
			q.push(q.front());
			q.pop();
		}
		cout << "Discarded cards: ";
		for(int i=0;i<dis.size();i++){
			if(i>0) cout << ", ";
			cout << dis[i];
		}
		cout << endl;
		cout << "Remaining card: " << q.front() << endl;
	}
}
