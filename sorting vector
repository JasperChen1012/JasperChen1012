#include <iostream>
#include <string>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    vector <string> a;
    vector <string> a1;
    vector <string> a2;
    vector <string> a3;
    string b;
    while(cin >> b){
        a.push_back(b);
        if(getchar()=='\n')
            break;
        }
    for(int i=0; i<a.size();i++){
        if((i+1)%3==1)
            a1.push_back(a[i]);
        else if((i+1)%3==2)
            a2.push_back(a[i]);
        else if((i+1)%3==0)
            a3.push_back(a[i]);
        }
        
    
    for(int j=0; j<a1.size();j++){
        cout << a1[j] << " ";
        }
    cout << endl;
    
    for(int k=0; k<a2.size();k++){
        cout << a2[k] << " ";
        }
   cout << endl;     
        
   for(int d=0; d<a3.size();d++){
        cout << a3[d] << " ";
        }
    cout << endl; 
    
    return 0;
}
