include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <bits/stdc++.h>

using namespace std;

int main(){
    unsigned long long int a, b, answer=0;
    cin >> a >> b;
    for(auto i=a; i<=b; i++) answer+=i;
    cout << answer << endl;
    
}
