#include <vector>
#include <iostream>
using namespace std;
 
int main(){
    int N;
    cin >> N;
    for(int i=0; i<N; i++){
        int a,b,s;
        vector <int> c;
        cin >> a >> b;
        for(int j=0; j<a; j++){
            cin >> s;
            c.push_back(s);
            }
        int ans=0;
        for(int k=0; k<b; k++) ans+=c[k];
        if(ans%2==0)
            cout << "No" << endl;
        else 
            cout << "Yes" << endl;
        }
    return 0;
    }
