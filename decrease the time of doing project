#pragma GCC optimize("03,unroll-loops")
#pragma GCC target("avx2,bmi,bmi2,lzcnt,popcnt")
#include <iostream>


using namespace std;

int main(){
    unsigned long long int a, b, answer=0;
    cin >> a >> b;
    for(auto i=a; i<=b; i++) answer+=i;
    cout << answer << endl;
    
}
